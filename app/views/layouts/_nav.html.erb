<header>
       
      <% if logged_in? %>
      <div class="header_logo">
        <!--ユーザ詳細に飛ばしたい-->
        <%= link_to embedded_svg("tabilog_logo.svg", class: "logo_svg"), user_path(current_user) %>
        <!--<= link_to "旅ログ", user_path(current_user) %>-->
      </div>
       
      <ul class="header_menus">
        <li><%= link_to 'Users', users_path %></li>
        <li><%= link_to 'Logout', logout_path, method: :delete %></li>
      </ul>  
      <% else %>
      <div class="header_logo">
        <%= link_to embedded_svg("tabilog_logo.svg", class: "logo_svg"), "/" %>
      </div>
       
        <ul class="header_menus">
          <li><%= link_to 'Signup', signup_path %></li>
          <li><%= link_to 'Login', login_path %></li>
        </ul>
       
      <% end %>  
</header>